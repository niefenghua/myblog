<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.z.model.Ctrs" table="ctrs" schema="blogpro">
        <id name="cid">
            <column name="CID" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="article">
            <column name="ARTICLE" sql-type="int(11)"/>
        </property>
        <property name="ipadress">
            <column name="IPADRESS" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="date">
            <column name="DATE" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>